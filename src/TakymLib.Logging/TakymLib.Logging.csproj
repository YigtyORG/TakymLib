<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<AssemblyName>TakymLib.Logging</AssemblyName>
		<RootNamespace>TakymLib.Logging</RootNamespace>
		<OutputType>Library</OutputType>
	</PropertyGroup>
	<Import Project="../../TakymLib.ProjectSettings.targets" />
	<PropertyGroup>
		<Product>TakymLib.Logging</Product>
		<Description>The common logging library.</Description>
		<PackageTags>$(PackageTags), Logging, Report</PackageTags>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging" Version="7.0.0" />
		<ProjectReference Include="../TakymLib/TakymLib.csproj" />
		<Compile Include="../TakymLib/Text/BuildString.cs" Link="Text/BuildString" />
	</ItemGroup>
</Project>
